{
  "name": "webp-equiv",
  "version": "1.0.0",
  "description": "A program for generating visually similar WebP images.",
  "main": "./dist/webp-equiv.js",
  "scripts": {
    "test": "npx mocha --require @babel/register ./test/test.mjs",
    "lint": "npx eslint --ext .mjs ./src/**",
    "clean": "npx rimraf dist",
    "build": "npm run clean && npx rollup -c"
  },
  "bin": {
    "webp-equiv": "./dist/cmd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malchata/webp-equiv.git"
  },
  "keywords": [
    "webp",
    "ssimulacra",
    "image",
    "optimization",
    "performance"
  ],
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/webp-equiv/issues"
  },
  "homepage": "https://github.com/malchata/webp-equiv#readme",
  "dependencies": {
    "cwebp-bin": "^4.0.0",
    "dwebp-bin": "^0.1.6",
    "imagemagick": "^0.1.3",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/register": "^7.0.0",
    "eslint": "^5.9.0",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.3"
  }
}
